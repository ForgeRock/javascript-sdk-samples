{
  "name": "custom-ui-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel build serve",
    "start:watch": "npm-run-all --parallel build:watch serve",
    "build": "webpack",
    "build:watch": "webpack --env.WATCH=true",
    "serve": "http-server . -c1 -p 9443 --ssl --cert ../../certs/forgerock-sdk.crt --key ../../certs/forgerock-sdk.key"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "dotenv": "^8.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "string-replace-loader": "^2.2.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@forgerock/javascript-sdk": "^1.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
